SELECT     MIN(t.title) AS typical_european_movie FROM     company_type AS ct,     info_type AS it,     movie_companies AS mc,     movie_info AS mi,     title AS t WHERE     ct.kind = 'production companies'     AND mc.note like '%%'     and mc.note like '%%'     AND mi.info IN (         'Color','USA:17 August 2004','Short','Canada:16+','English','17','Short','RAT:1.85 : 1','PFM:35 mm'     )     AND t.production_year > 2007     AND t.id = mi.movie_id     AND t.id = mc.movie_id     AND mc.movie_id = mi.movie_id     AND ct.id = mc.company_type_id     AND it.id = mi.info_type_id;
