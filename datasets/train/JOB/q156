SELECT     MIN(t.title) AS movie_title FROM     keyword AS k,     movie_info AS mi,     movie_keyword AS mk,     title AS t WHERE     k.keyword like '%sequel%'     AND mi.info IN (         'English','UK:4 June 2009','USA','Short','OFM:35 mm','USA:6 February 2007','Sport'     )     AND t.production_year > 2007     AND t.id = mi.movie_id     AND t.id = mk.movie_id     AND mk.movie_id = mi.movie_id     AND k.id = mk.keyword_id;
