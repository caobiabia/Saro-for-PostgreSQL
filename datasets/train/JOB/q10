SELECT     MIN(t.title) AS movie_title FROM     keyword AS k,     movie_info AS mi,     movie_keyword AS mk,     title AS t WHERE     k.keyword like '%sequel%'     AND mi.info IN (         'Color','PCS:Spherical','12','West Germany','USA','USA:8 October 2006','Germany:18'     )     AND t.production_year > 2011     AND t.id = mi.movie_id     AND t.id = mk.movie_id     AND mk.movie_id = mi.movie_id     AND k.id = mk.keyword_id;
