SELECT     MIN(t.title) AS movie_title FROM     keyword AS k,     movie_info AS mi,     movie_keyword AS mk,     title AS t WHERE     k.keyword like '%sequel%'     AND mi.info IN (         'Germany:2 February 2011','CAM:Sony PMW-EX1','Drama','OFM:35 mm','USA','UK'     )     AND t.production_year > 1993     AND t.id = mi.movie_id     AND t.id = mk.movie_id     AND mk.movie_id = mi.movie_id     AND k.id = mk.keyword_id;
